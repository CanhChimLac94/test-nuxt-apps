{
  "name": "apps",
  "version": "1.0.0",
  "private": true,
  "global": true,
  "scripts": {
    "dev": "nuxt --config-file apps/live-chat/nuxt.config.js",
    "build": "nuxt build ",
    "start": "nuxt start --config-file apps/live-chat/nuxt.config.js",
    "generate": "nuxt generate",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "build-default": "nuxt build --config-file apps/default/nuxt.config.js",
    "start-default": "nuxt start --config-file apps/default/nuxt.config.js",
    "generate-default": "nuxt generate --config-file apps/default/nuxt.config.js",
    "export-default": "nuxt export --config-file apps/default/nuxt.config.js",
    "serve-default": "nuxt serve --config-file apps/default/nuxt.config.js",
    "publish": "npm run generate-default && npm run copy-default",
    "copy": "coppy.bat html_\\default",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "lint": "npm run lint:js && npm run lint:style",
    "test:e2e": "nightwatch"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.15.1",
    "husky": "^8.0.3",
    "nuxt": "^2.15.7",
    "pixi.js": "^6.2.1",
    "sass-loader": "^10.1.1",
    "socket.io-client": "^2.3.0",
    "uuid": "^8.3.2",
    "vis-data": "^7.1.4",
    "vis-network": "^9.1.2",
    "vuetify": "^2.5.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "chromedriver": "^91.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "lint-staged": "^10.5.4",
    "nightwatch": "^1.7.6",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^4.45.0"
  }
}
